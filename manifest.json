{
  "manifest_version": 3,
  "name": "Hide YouTube Members Only Videos",
  "version": "1.0",
  "description": "Hides all members only videos from recommendations on YouTube.",
  "developer": {
    "name": "Matt Lahti",
    "url": "https://github.com/mattlahti"
  },
  "homepage_url": "https://github.com/mattlahti/hide-members-only-videos",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "64": "assets/icon-64.png",
    "96": "assets/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "exclude_matches": [
        "*://*.music.youtube.com/*"
      ],
      "js": [
        "dist/contentscript.bundle.js"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "64": "assets/icon-64.png",
      "96": "assets/icon-96.png"
    },
    "default_title": "Hide Members Only Videos",
    "default_popup": "popup/stats.html"
  },
  "permissions": [
    "storage"
  ]
}
